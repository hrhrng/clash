{
  "name": "master-clash",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered video production platform",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules .turbo"
